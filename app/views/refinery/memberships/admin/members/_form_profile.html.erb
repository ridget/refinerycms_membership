<fieldset id="member_profile_form">
  <legend><%=t('profile', :scope => 'refinery.admin.members')%></legend>
  <p class="field">
      <%= f.label :first_name %>
      <%= f.text_field :first_name, :class => 'widest' %>
  </p>
 
  <p class="field">
      <%= f.label :last_name %>
      <%= f.text_field :last_name, :class => 'widest' %>
  </p>
 
  <p class="field">
      <%= f.label :phone %>
      <%= f.text_field :phone, :class => 'widest' %>
  </p>
</fieldset>
